{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "4.0.4",
    "craftcms/cms": "5.0.5",
    "craftcms/feed-me": "6.0.0",
    "craftcms/postmark": "3.1.0",
    "craftpulse/craft-colour-swatches": "^5.0.0",
    "nystudio107/craft-minify": "5.0.0",
    "nystudio107/craft-retour": "5.0.0",
    "nystudio107/craft-seomatic": "5.0.2",
    "nystudio107/craft-typogrify": "5.0.0",
    "putyourlightson/craft-blitz": "5.2.0",
    "spacecatninja/imager-x": "5.0.0",
    "spicyweb/craft-embedded-assets": "5.1.0",
    "verbb/formie": "3.0.0-beta.9",
    "verbb/hyper": "2.0.0-beta.8",
    "verbb/icon-picker": "3.0.0-beta.3",
    "verbb/navigation": "3.0.0-beta.3",
    "verbb/shortcodes": "4.0.0-beta.1",
    "verbb/social-share": "2.0.0-beta.1",
    "verbb/tablemaker": "5.0.0-beta.2",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}