{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "4.0.6",
    "craftcms/cms": "5.1.4",
    "craftcms/feed-me": "6.0.1",
    "craftcms/postmark": "3.1.0",
    "craftpulse/craft-colour-swatches": "^5.0.0",
    "nystudio107/craft-minify": "5.0.0",
    "nystudio107/craft-retour": "5.0.0",
    "nystudio107/craft-seomatic": "5.0.2",
    "nystudio107/craft-typogrify": "5.0.0",
    "putyourlightson/craft-blitz": "5.3.1",
    "spacecatninja/imager-x": "5.0.0",
    "spacecatninja/imager-x-power-pack": "1.0.3",
    "spicyweb/craft-embedded-assets": "5.1.0",
    "verbb/buttonbox": "5.0.0",
    "verbb/field-manager": "4.0.0",
    "verbb/formie": "3.0.0-beta.11",
    "verbb/hyper": "2.0.0",
    "verbb/icon-picker": "3.0.0",
    "verbb/navigation": "3.0.0",
    "verbb/shortcodes": "4.0.0",
    "verbb/social-share": "2.0.0",
    "verbb/tablemaker": "5.0.0",
    "vlucas/phpdotenv": "^5.4.0",
    "wmd/section-and-product-type": "2.0.8"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}